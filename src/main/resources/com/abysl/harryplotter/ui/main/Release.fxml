<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="10.0"
      xmlns="http://javafx.com/javafx/16" fx:controller="com.abysl.harryplotter.ui.main.ReleaseView">
  <children>
    <Label fx:id="releaseVersion" text="Release"/>
    <TextArea fx:id="changeLog" editable="false" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS"/>
    <Label text="Would you like to download it?"/>
    <HBox alignment="CENTER" spacing="10.0">
      <children>
        <Button minWidth="75.0" mnemonicParsing="false" onAction="#onCancel" text="Cancel"/>
        <Button minWidth="75.0" mnemonicParsing="false" onAction="#onDownload" text="Download"/>
      </children>
    </HBox>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
  </padding>
</VBox>

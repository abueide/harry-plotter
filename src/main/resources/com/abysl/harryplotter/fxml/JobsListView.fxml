<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxWidth="350.0" prefHeight="650.0" prefWidth="350.0" spacing="10.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.abysl.harryplotter.view.JobsListView">
  <children>
    <ListView fx:id="jobsView" maxWidth="350.0" minWidth="200.0" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
    <HBox alignment="CENTER" layoutX="10.0" layoutY="635.0" spacing="10.0">
      <children>
        <Button mnemonicParsing="false" onAction="#onClear" text="Clear Temp Dirs" />
        <Button minWidth="100.0" mnemonicParsing="false" onAction="#onStopAll" text="Stop All" />
        <Button minWidth="100.0" mnemonicParsing="false" onAction="#onStartAll" text="Start Stagger" />
      </children>
    </HBox>
  </children>
</VBox>
